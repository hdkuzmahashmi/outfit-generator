<!DOCTYPE html>
<html lang="zxx">
<head>

    <!-- Meta Tag -->
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name='copyright' content=''>
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>@ViewBag.Title Outfit Generator</title>
    <!-- Web Font -->
    <link href="https://fonts.googleapis.com/css?family=Poppins:200i,300,300i,400,400i,500,500i,600,600i,700,700i,800,800i,900,900i&display=swap" rel="stylesheet">

    <!-- StyleSheet -->
    <!-- Bootstrap -->
    <link rel="stylesheet" href="@Url.Content("~/UI/css/bootstrap.css")">
    <!-- Magnific Popup -->
    <link rel="stylesheet" href="@Url.Content("~/UI/css/magnific-popup.min.css")">
    <!-- Font Awesome -->
    <link rel="stylesheet" href="@Url.Content("~/UI/css/font-awesome.css")">
    <!-- Fancybox -->
    <link rel="stylesheet" href="@Url.Content("~/UI/css/jquery.fancybox.min.css")">
    <!-- Themify Icons -->
    <link rel="stylesheet" href="@Url.Content("~/UI/css/themify-icons.css")">
    <!-- Nice Select CSS -->
    <link rel="stylesheet" href="@Url.Content("~/UI/css/niceselect.css")">
    <!-- Animate CSS -->
    <link rel="stylesheet" href="@Url.Content("~/UI/css/animate.css")">
    <!-- Flex Slider CSS -->
    <link rel="stylesheet" href="@Url.Content("~/UI/css/flex-slider.min.css")">
    <!-- Owl Carousel -->
    <link rel="stylesheet" href="@Url.Content("~/UI/css/owl-carousel.css")">
    <!-- Slicknav -->
    <link rel="stylesheet" href="@Url.Content("~/UI/css/slicknav.min.css")">

    <!-- Eshop StyleSheet -->
    <link rel="stylesheet" href="@Url.Content("~/UI/css/reset.css")">
    <link rel="stylesheet" href="@Url.Content("~/UI/style.css")">
    <link rel="stylesheet" href="@Url.Content("~/UI/css/responsive.css")">
    <link rel="stylesheet" href="CustomStyle.css">

</head>
<body class="js">
	
	<!-- Preloader -->
	<div class="preloader">
		<div class="preloader-inner">
			<div class="preloader-icon">
				<span></span>
				<span></span>
			</div>
		</div>
	</div>
	<!-- End Preloader -->
	
    <div class="wrapper">
        <nav class="navbar navbar-default" role="navigation">
            <div class="navbar-header navbar-header-center">
                <a class="navbar-brand" href="#"><img src="~/UI/images/web-logo.png"></a>
            </div>
            <div class="clearfix"></div>
        </nav>

        <!---start slider image section----->
        <div class="container-fulid" id="slide-container">
            <div class="section">

                <div class="row" id="hero-slider">
                    <div class="col-sm-6">
                        <div class="first sec">
                            <!-- <img class="sec-icon" src="https://via.placeholder.com/100x100"/> -->
                            <h4>Welcome to</h4>
                            <div class="col-sm-6" id="animated"><h1 class="maintitle" data-text="Outfit Generator">Outfit Generator</h1></div>
                            <div class="svg">
                                <h5>Choose Segment</h5><svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-arrow-right" viewBox="0 0 16 16">
                                <path fill-rule="evenodd" d="M1 8a.5.5 0 0 1 .5-.5h11.793l-3.147-3.146a.5.5 0 0 1 .708-.708l4 4a.5.5 0 0 1 0 .708l-4 4a.5.5 0 0 1-.708-.708L13.293 8.5H1.5A.5.5 0 0 1 1 8z" />
                                </svg>
                            </div>
                        </div>
                    </div>
                    <div class="col-sm-6">
                        <div class="second sec">
                            <div class="sliders-cont">
                                <div id="slider" class=" col-sm-6">
                                    <a href="@Url.Action("GetWomenCategories","Category")">
                                        <ul>
                                            <li class="selfie-img"><img src="~/UI/images/damen_kruz.jpg"></li>
                                        </ul>
                                        <h2 id="heading-main-page">Women</h2>
                                    </a>
                                </div>
                                <div id="slider" class=" col-sm-6">
                                    <a href="@Url.Action("GetManCategories","Category")">
                                        <ul>
                                            <li class="selfie-img"><img src="~/UI/images/herren_kurz.jpg"></li>
                                        </ul>
                                        <h2 id="heading-main-page">Men</h2>
                                    </a>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

        </div>
        <!---end slider image section----->
     </div>

    <script src='@Url.Content("~/UI/js/jquery.min.js")'></script>
    <script src='@Url.Content("~/UI/js/jquery-migrate-3.0.0.js")'></script>
    <script src='@Url.Content("~/UI/js/jquery-ui.min.js")'></script>
    <!-- Popper JS -->
    <script src='@Url.Content("~/UI/js/popper.min.js")'></script>
    <!-- Bootstrap JS -->
    <script src='@Url.Content("~/UI/js/bootstrap.min.js")'></script>
    <!-- Color JS -->
    @*<script src='@Url.Content("~/UI/js/colors.js")'></script>*@
    <!-- Slicknav JS -->
    <script src='@Url.Content("~/UI/js/slicknav.min.js")'></script>
    <!-- Owl Carousel JS -->
    <script src='@Url.Content("~/UI/js/owl-carousel.js")'></script>
    <!-- Magnific Popup JS -->
    <script src='@Url.Content("~/UI/js/magnific-popup.js")'></script>
    <!-- Waypoints JS -->
    <script src='@Url.Content("~/UI/js/waypoints.min.js")'></script>
    <!-- Countdown JS -->
    <script src='@Url.Content("~/UI/js/finalcountdown.min.js")'></script>
    <!-- Nice Select JS -->
    <script src='@Url.Content("~/UI/js/nicesellect.js")'></script>
    <!-- Flex Slider JS -->
    <script src='@Url.Content("~/UI/js/flex-slider.js")'></script>
    <!-- ScrollUp JS -->
    <script src='@Url.Content("~/UI/js/scrollup.js")'></script>
    <!-- Onepage Nav JS -->
    <script src='@Url.Content("~/UI/js/onepage-nav.min.js")'></script>
    <!-- Easing JS -->
    <script src='@Url.Content("~/UI/js/easing.js")'></script>
    <!-- Active JS -->
    <script src='@Url.Content("~/UI/js/active.js")'></script>

</body>
</html>